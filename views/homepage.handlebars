<h1>Homepage</h1>
{{#each blogs as |blog|}}      
      <!-- BLOG CARD -->
      <div class="card-holder">
        <div class="card-left">
          <div class="title-container">
            <h3 class="title">{{blog.title}}</h3>
          </div>
          <div class="date-container">
            <div>{{blog.date}}</div>
          </div>
        </div>
        <div class="card-right">
          <h4>{{blog.}}</h4>
        </div>
      </div>
{{/each}}
