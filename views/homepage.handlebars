{{#each posts}}      
    <!-- BLOG CARD -->
    <div class="card-holder">
      <div class="card-left">
        <div class="title-container">
          <h3 class="title">{{title}}</h3>
        </div>
        <div class="date-container">
          <div>{{date_created}}</div>
        </div>
      </div>
      <div class="card-right">
        <h4>{{user.username}}</h4>
      </div>
    </div>
{{/each}}
